{
  "manifest_version": 2,

  "name": "Nexpro Quick Digester",
  "description": "This extension will analyze a page using GTmetrix",
  "version": "1.0",
  "icons": {
      "16": "assets/images/icon.png",
      "48": "assets/images/icon.png",
      "128": "assets/images/icon.png"
  },

  "browser_action": {
   "default_popup": "main.html"
  },
  "permissions": [
   "activeTab"
   ]
}
